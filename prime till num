#include <stdio.h>
int n, flag;
int main() {
    printf("Please enter the number till which you want to print out prime numbers : ");
    scanf(" %d", &n);
    for (int i = 2; i <= n; i++) {
        for (int j = 2; j<=i/2; j++) {
            flag = 0;
            if (i%j == 0) {
                flag = 1;
                break;
            }
        }
        if (flag == 0) {
            printf("%d \n", i);
        }
    }
}
